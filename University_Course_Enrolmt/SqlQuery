CREATE TABLE COURSE_TBL (
    COURSE_ID VARCHAR2(10) PRIMARY KEY,
    COURSE_TITLE VARCHAR2(150) NOT NULL,
    CREDITS NUMBER(2) CHECK (CREDITS > 0),
    CAPACITY NUMBER(4) CHECK (CAPACITY > 0),
    ENROLLED_COUNT NUMBER(4)
);
INSERT INTO COURSE_TBL VALUES ('CSE101','Introduction to Computer Science',4,120,95);
INSERT INTO COURSE_TBL VALUES ('MAT201','Calculus II',3,80,80);
INSERT INTO COURSE_TBL VALUES ('PHY150','Physics I',3,60,45);
COMMIT;
Create ENROLLMENT_TBL
CREATE TABLE ENROLLMENT_TBL (
    ENROLLMENT_ID NUMBER(8) PRIMARY KEY,
    COURSE_ID VARCHAR2(10),
    STUDENT_ID VARCHAR2(12),
    STUDENT_NAME VARCHAR2(100),
    ENROLLMENT_DATE DATE,
    STATUS VARCHAR2(10),
    CONSTRAINT fk_course FOREIGN KEY (COURSE_ID)
    REFERENCES COURSE_TBL(COURSE_ID)
);
